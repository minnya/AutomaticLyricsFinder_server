{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: FastAPI (Uvicorn)",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--port","3010",
          "--reload"
        ],
        "cwd": "${workspaceFolder}/app",
        "jinja": true,
        "envFile": "${workspaceFolder}/.env",
        "env": {
          "MARIADB_HOST": "localhost",
      }
      }
    ]
  }
  